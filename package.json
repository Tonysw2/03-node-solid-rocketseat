{
  "name": "03-node-solid-rocketseat",
  "version": "1.0.0",
  "description": "GymPass style app",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "biome:check": "biome check src --write",
    "test:create-prisma-env": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-env": "npm link vitest-environment-prisma",
    "pretest:e2e": "npm run test:create-prisma-env && npm run test:install-prisma-env",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:e2e": "vitest run --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.9.1",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "^22.5.5",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.1.1",
    "@vitest/ui": "2.1.1",
    "prisma": "5.19.1",
    "supertest": "7.0.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.1"
  },
  "dependencies": {
    "@fastify/jwt": "9.0.0",
    "@prisma/client": "5.19.1",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "fastify": "^5.0.0",
    "zod": "3.23.8"
  }
}
